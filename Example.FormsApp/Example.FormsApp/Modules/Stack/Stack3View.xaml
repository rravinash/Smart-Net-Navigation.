<?xml version="1.0" encoding="UTF-8"?>
<ContentView x:Class="Example.FormsApp.Modules.Stack.Stack3View"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:resolver="clr-namespace:Smart.Forms.Resolver;assembly=Smart.Forms"
             xmlns:markup="clr-namespace:Smart.Forms.Markup;assembly=Smart.Forms"
             xmlns:stack="clr-namespace:Example.FormsApp.Modules.Stack;assembly=Example.FormsApp"
             xmlns:views="clr-namespace:Example.FormsApp.Views;assembly=Example.FormsApp"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type={x:Type stack:Stack3ViewModel}}"
             resolver:BindingContextResolver.Type="{x:Type stack:Stack3ViewModel}"
             views:ViewProperty.Title="Stack3">
  <StackLayout>
        <Button Text="Pop" Style="{StaticResource NavigationButton}"
            Command="{Binding Pop}" CommandParameter="{markup:Int32 1}"/>
        <Button Text="Pop2" Style="{StaticResource NavigationButton}"
            Command="{Binding Pop}" CommandParameter="{markup:Int32 2}"/>
  </StackLayout>
</ContentView>