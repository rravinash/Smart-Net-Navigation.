<UserControl x:Class="Example.WindowsApp.Views.MenuView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:resolver="clr-namespace:Smart.Windows.Resolver;assembly=Smart.Windows"
             xmlns:local="clr-namespace:Example.WindowsApp.Views"
             xmlns:stack="clr-namespace:Example.WindowsApp.Views.Stack"
             xmlns:wizard="clr-namespace:Example.WindowsApp.Views.Wizard"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type={x:Type local:MenuViewModel}}"
             resolver:DataContextResolver.Type="{x:Type local:MenuViewModel}"
             d:DesignHeight="320" d:DesignWidth="480">
    <Grid>
        <Button Content="Stack" HorizontalAlignment="Left" Margin="233,84,0,0" VerticalAlignment="Top" Width="75"
                Command="{Binding Forward}" CommandParameter="{x:Type stack:Stack1View}"/>
        <Button Content="Wizard" HorizontalAlignment="Left" Margin="233,125,0,0" VerticalAlignment="Top" Width="75"
            Command="{Binding Forward}" CommandParameter="{x:Type wizard:WizardInput1View}"/>

    </Grid>
</UserControl>
